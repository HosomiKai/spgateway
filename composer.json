{
    "name": "hosomikai/spgateway",
    "description": ":SPGateway - Laravel 5 version",
    "license": "license",
    "authors": [
        {
            "name": "hosomikai",
            "email": "showking111@gmail.com",
            "homepage": "https://github.com/HosomiKai"
        }
    ],
    "homepage": "https://github.com/hosomikai/spgateway",
    "keywords": ["Laravel", "spgateway"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "hosomikai\\spgateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hosomikai\\spgateway\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "hosomikai\\spgateway\\spgatewayServiceProvider"
            ],
            "aliases": {
                "SPGateway": "hosomikai\\spgateway\\Facades\\SPGateway"
            }
        }
    }
}
